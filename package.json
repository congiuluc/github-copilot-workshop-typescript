{
  "name": "github-copilot-typescript-workshop",
  "version": "1.0.0",
  "description": "A comprehensive workshop for learning all GitHub Copilot features while building a TypeScript task management application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docs:generate": "typedoc src",
    "docs:serve": "http-server docs",
    "clean": "rimraf dist coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "github-copilot",
    "typescript",
    "workshop",
    "ai-development",
    "task-management",
    "tutorial"
  ],
  "author": "Workshop Creator",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "@types/express": "^4.17.17",
    "cors": "^2.8.5",
    "@types/cors": "^2.8.13",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.2",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "redis": "^4.6.7",
    "ws": "^8.13.0",
    "@types/ws": "^8.5.5"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "@types/node": "^20.4.5",
    "ts-node": "^10.9.1",
    "jest": "^29.6.2",
    "@types/jest": "^29.5.3",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12",
    "@faker-js/faker": "^8.0.2",
    "jest-extended": "^4.0.1",
    "eslint": "^8.45.0",
    "@typescript-eslint/parser": "^6.2.1",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "prettier": "^3.0.0",
    "typedoc": "^0.24.8",
    "rimraf": "^5.0.1",
    "http-server": "^14.1.1",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/github-copilot-typescript-workshop.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/github-copilot-typescript-workshop/issues"
  },
  "homepage": "https://github.com/your-org/github-copilot-typescript-workshop#readme"
}
